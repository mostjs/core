    "build": "rimraf lib dist && rollup -c && uglifyjs dist/mostCore.js -c \"warnings=false\" -m -o dist/mostCore.min.js",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mostjs/core"
  },
  "keywords": [
    "reactive",

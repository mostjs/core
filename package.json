  "license": "MIT",
  "devDependencies": {
    "@most/eslint-config-most": "^1.0.3",
    "buba": "^4.0.2",
    "buster": "^0.7.18",
    "eslint": "^3.12.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.38.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "@most/prelude": "^1.4.0"
  }

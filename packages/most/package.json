{
  "name": "most",
  "version": "2.0.0",
  "description": "Reactive programming with lean, functions-only, curried, tree-shakeable API",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:flow",
    "test:lint": "standard --fix 'src/**/*.js' 'test/**/*.js' --verbose | snazzy",
    "test:flow": "flow check",
    "build": "npm run build:dist && npm run build:flow",
    "build:dist": "rollup -c",
    "build:flow": "cpy src/index.{d.ts,js.flow} dist",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mostjs/core"
  },
  "keywords": [
    "reactive",
    "reactive programming",
    "events",
    "event stream",
    "promise",
    "promises",
    "monad",
    "monadic",
    "functional",
    "async"
  ],
  "author": "brian@hovercraftstudios.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-annotate-pure-calls": "^0.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.0.5",
    "flow-bin": "^0.57.3",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "@most/core": "^1.3.2",
    "@most/disposable": "^1.2.1",
    "@most/scheduler": "^1.2.1",
    "@most/types": "^1.0.1"
  },
  "standard": {
    "ignore": [
      "test/perf",
      "test/flow"
    ]
  }
}
